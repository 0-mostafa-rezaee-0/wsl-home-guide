# Ignore everything by default
*

# Track README.md
!README.md

# Track banner.png
!banner.png

# Track shell configurations
!.bashrc
!.zshrc
!.profile
!.bash_logout
!.shell.pre-oh-my-zsh

# Track Git configurations
!.gitconfig
!.gitignore_global

# Track editor configurations
!.vimrc
!.viminfo

# Track important dotfiles
!.inputrc

# Don't track sensitive or machine-specific files
.git-credentials
.ssh/
.aws/
.azure/
.docker/

# Don't track history files
.bash_history
.zsh_history
.python_history
.lesshst
.wget-hsts

# Don't track cache files
.cache/
.npm/
.yarn/

# Include the .gitignore itself
!.gitignore 